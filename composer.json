{
    "name": "genotipo/dulceriasdenny.com",
    "type": "project",
    "description": "Dulcerías Denny",
    "homepage": "https://dulceriasdenny.com/",
    "license": "Genotipo (R)",
    "require": {
        "php": "^7.4 || ^8.0",
        "codeigniter4/framework": "^4.0",
        "codeigniter4/settings": "^2.1",
        "codeigniter4/translations": "^4.3",
        "imangazaliev/didom": "^2.0",
        "shuchkin/simplexlsxgen": "^1.3",
        "tinify/tinify": "^1.6"
    },
    "require-dev": {
        "codeigniter/coding-standard": "^1.7",
        "fakerphp/faker": "^1.9",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "scripts": {
        "test": "phpunit",
        "prettier": "vendor/bin/php-cs-fixer fix --diff",
        "docs": "gendoc docs/index.html src/docs/config.xml"
    },
    "support": {
        "forum": "https://forum.codeigniter.com/",
        "source": "https://github.com/codeigniter4/CodeIgniter4",
        "slack": "https://codeigniterchat.slack.com"
    },
    "authors": [
        {
            "name": "Francisco Javier Solís Martínez",
            "email": "franciscosolism08@gmail.com",
            "homepage": "https://github.com/francisco-solis99",
            "role": "Frontend Developer"
        },
        {
            "name": "Ricardo García Jiménez",
            "email": "ricardogj08@riseup.net",
            "homepage": "https://cawfee.club/ricardogj08",
            "role": "Backend Developer"
        }
    ]
}
